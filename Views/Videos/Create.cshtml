@using UranusAdmin.Dto
@model VideoDto
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<form method="post" asp-action="Create" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Title">Title</label>
        <input asp-for="Title" class="form-control" placeholder="Enter title" />
        <span asp-validation-for="Title" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="VideoContent">File</label>
        <input asp-for="VideoContent" type="file" class="form-control" placeholder="Enter a file" />
        <span asp-validation-for="VideoContent" class="text-danger"></span>
    </div>
    <br />
    <button class="btn btn-primary" type="submit">Submit</button>
</form>